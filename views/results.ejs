<html>
<head>
<style>
 body {
        background-color: CBECF6;
        font-family: 'Ubuntu', sans-serif;
    }
    
    .main {
        background-color: #FFFFFF;
        width: 400px;
        height: 400px;
        margin: 7em auto;
        border-radius: 1.5em;
        box-shadow: 0px 11px 35px 2px rgba(0, 0, 0, 0.14);
    }
    

.sidenav {
    height: 100%;
    width: 300px;
    position: fixed;
    z-index: 1;
    top: 0;
    left: 0;
    background-color: #111;
    overflow-x: hidden;
    padding-top: 20px;
}

.sidenav a {
    padding: 6px 8px 6px 16px;
    text-decoration: none;
    font-size: 25px;
    color: #818181;
    display: block;
}

.sidenav a:hover {
    color: white;
}

li.margin{
margin-left:700px;
}
li.unstyled{
list-style-type: none;

}
</style>
</head>

<body bgcolor="CBECF6">
<p><span id="span"></span></p>
<div class="sidenav">
  <br>
  <a href='/search'>Search Movie</a>
  <br>
  

</div>

<h1 align="center">Welcome To The Movie World!</h1>

<% data["Search"].forEach(function(movie){ %>

<li class='margin'>

<h2><%=movie["Title"]%> - <%=movie["Year"]%></h2>

</li>
<li class='margin' class="unstyled">
<img src='<%=movie["Poster"]%>'>;
</li>

<% }) %>

<a align='center' href="/">Search Again<a/>
</body>
</html>